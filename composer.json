{
	"name": "glhd/laralint",
	"description": "Laravel linting framework",
	"keywords": [
		"laravel",
		"lint",
		"linting"
	],
	"authors": [
		{
			"name": "Chris Morrell",
			"homepage": "http://www.cmorrell.com"
		}
	],
	"license": "MIT",
	"require": {
		"php": ">=7.3.0",
		"microsoft/tolerant-php-parser": "dev-main",
		"illuminate/support": "^6.0|^7.0|^8.0|^9.0",
		"illuminate/console": "^6.0|^7.0|^8.0|^9.0"
	},
	"require-dev": {
		"laravel/framework": "^6.0|^7.0|^8.0|^9.0",
		"orchestra/testbench": "^4.0|^5.0|^7.0",
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"Glhd\\LaraLint\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		],
		"psr-4": {
			"Glhd\\LaraLint\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "stable",
	"scripts": {
		"test": "phpunit"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Glhd\\LaraLint\\Support\\LaraLintServiceProvider"
			]
		}
	}
}
